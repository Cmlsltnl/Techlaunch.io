{
  "name": "techlaunch.io",
  "version": "1.0.0",
  "description": "Website for the Techlaunch coding school at the Florida Vocational Institute",
  "main": "js/index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "prod": "webpack -p",
    "sass": "node-sass css -o css",
    "autoprefixer": "postcss css/**/*.css --use autoprefixer -r",
    "build-css": "npm run sass && npm run autoprefixer",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "cleancss": "cleancss css/**/*.css",
    "build-css-min": "npm run sass && npm run autoprefixer && npm run cleancss"
  },
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "postcss-cli": "^4.0.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "lodash.throttle": "^4.1.1"
  }
}
