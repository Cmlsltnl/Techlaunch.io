{
  "name": "techlaunch.io",
  "version": "1.0.0",
  "description": "Website for the Techlaunch coding school at the Florida Vocational Institute",
  "main": "js/index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "prod": "webpack -p",
    "sass": "node-sass css -o css",
    "sass:watch": "node-sass -w css -o css",
    "autoprefixer": "postcss css/**/*.css --use autoprefixer -r",
    "build-css": "npm run sass && npm run autoprefixer",
    "cleancss": "cleancss css/**/*.css",
    "build-css-min": "npm run sass && npm run autoprefixer && npm run cleancss"
  },
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "clean-css-cli": "^4.1.5",
    "dotenv": "^4.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "postcss-cli": "^4.1.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "carousel-slider": "^1.1.0",
    "jquery": "^3.2.1",
    "lodash.throttle": "^4.1.1"
  }
}
